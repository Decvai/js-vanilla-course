{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true,
	},
	"rules": {
		"no-tabs": 0,
		"indent": "off",
		"require-jsdoc": 0,
		"object-curly-spacing": 0,
		"arrow-parens": 0,
		"no-unused-vars": "warn",
		"operator-linebreak": "off",
		"linebreak-style": "off",
		"space-before-function-paren": "off"
	},
	"extends": [
		"eslint:recommended",
		"google"
	],
}